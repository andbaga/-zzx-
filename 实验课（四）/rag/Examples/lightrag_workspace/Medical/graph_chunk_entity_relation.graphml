<node id="Stage 4A">
  <data key="d0">Stage 4A</data>
  <data key="d1">concept</data>
  <data key="d2">Stage 4A ovarian cancer is characterized by the presence of cancer cells in the fluid around the lungs, known as a malignant pleural effusion.</data>
  <data key="d3">chunk-b69d38fafe014cece3427bd0459ae42b</data>
  <data key="d4">unknown_source</data>
  <data key="d5">1767249526</data>
